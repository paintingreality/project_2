<h1>Posts#edit</h1>
<p>Find me in app/views/posts/edit.html.erb</p>

<%= form_for @post do |p| %>
  <%= p.text_field :make, placeholder: "Make" %>
  <%= p.text_field :model, placeholder: "Model" %>
  <%= p.file_field :cars, placeholder: "Picture" %>
  <%= p.text_field :year, placeholder: "Year" %>
  <%= p.submit %>
<% end %>
